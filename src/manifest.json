{
    "manifest_version": 2,
    "name": "Kittify",
    "version": "1.0",
    "description": "Replace images with photos of cute kittens",
    "icons": {
        "128": "icons/128x128.png"
    },
    "content_scripts": [
        {
            "js": [
                "kittify.js"
            ],
            "matches": [
                "*://*/*"
            ],
            "run_at": "document_end"
        }
    ]
}